U"LSHIFT"       : U"LSHIFT"; # 
# U"Z"            : U"Z"; # À
# U"X"            : U"X"; # Y
# U"C"            : U"C"; # X
# U"V"            : U"V"; # .
# U"B"            : U"B"; # K
U"FUNCTION2"    : U"BACKSLASH"; # Ç

### LEFT ROW 5 ###
U"LGUI"         : U"LCTRL"; # 
U"BACKTICK"     : U"LCTRL" + U"H"; # Ctrl+C: Copy
U"FUNCTION3"    : U"LCTRL" + U"U"; # Ctrl+V: Past
U"FUNCTION4"    : U"LCTRL" + U"LBRACE"; # Ctrl+Z: Cancel
U"FUNCTION5"    : U"LALT"; # 

### LEFT THUMB BLOCK ###
U"LCTRL"        : None; #
U"LALT"         : U"NEXT LAYER"; #
U"HOME"         : None; #
U"BACKSPACE"    : U"SPACE"; # 
U"DELETE"       : U"FUN1"; # 
U"END"          : U"LGUI"; # 

