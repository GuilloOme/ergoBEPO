# U"2"            : U"2"; # 
# U"3"            : U"3"; # 
# U"4"            : U"4"; # 
# U"5"            : U"5"; # 
U"ESC"          : U"BACKTICK"; # $

### LEFT ROW 2 ###
U"BACKSLASH"    : U"TAB";  # 
# U"Q"            : U"Q"; # B
# U"W"            : U"W"; # É
# U"E"            : U"E"; # P
# U"R"            : U"R"; # O
# U"T"            : U"T"; # È
U"FUNCTION1"    : U"RBRACE"; # W

### LEFT ROW 3 ###
U"TAB"          : U"FUN1"; # function 1
